<swiper class="swiper {{isHeight?'height':''}}" 
      indicator-dots
      indicator-active-color="deeppink"
      autoplay circular>
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image class="swiper-img" src="{{item}}" mode="widthFix"></image>
    </swiper-item>
  </block>
</swiper>     
  